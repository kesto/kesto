<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>Kesto Control</title>
    <link type="text/css" rel="stylesheet" media="all" href="/kesto/ui/style.css" />
</head>

<!-- when the page finishes loading, start filling in details -->
<body>

    <!-- header with logo -->
    <div id="header">
        <a id="kesto-logo" href="http://www.conversion.co.jp" target="_blank"></a>
    </div><!-- header -->


	<div class="menu">
		<table class="menu_table">
			<tr class="menu_table_tr">
				<td class="menu_table_td">
					<div id="menu_dashboard" class="menu_content menu_icon"/>
					<div id="menu_dashboard_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">DashBoard</>
				</td>
				<td class="menu_table_td">
					<div id="menu_event" class="menu_content menu_icon"/>
					<div id="menu_event_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">Event</>
				</td>
				<td class="menu_table_td">
					<div id="menu_job" class="menu_content menu_icon"/>
					<div id="menu_job_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">Job</>
				</td>
			</tr>
			<tr class="menu_table_tr">
				<td class="menu_table_td">
					<div id="menu_report" class="menu_content menu_icon"/>
					<div id="menu_report_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">Report</>
				</td>
				<td class="menu_table_td">
					<div id="menu_analyze" class="menu_content menu_icon"/>
					<div id="menu_analyze_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">Analyze</>
				</td>
				<td class="menu_table_td">
					<div id="menu_setting" class="menu_content menu_icon"/>
					<div id="menu_setting_image" class="menu_icon_image"/>
					<div class="menu_icon_txt">Setting</>
				</td>
			</tr>
		</table>
	
		<div id="sign_in">
			<table>
				<tr>
					<td>
						<div id="sign_in_header">
							<span class="gui-button-msg">Sign in</span>
						</div>
					</td>
				</tr>
				<tr>
					<td>
						<div class="gui-field gui-text">
							<input id="sign_in_userid" class="gui-field-input gui-text" type="text" name="userid" />
						</div>
					</td>
		        </tr>
		        <tr>
					<td>
						<div class="gui-field gui-text">
							<input id="sign_in_password" class="gui-field-input gui-text" type="password" name="password" />
						</div>
					</td>
				</tr>
				<tr>
					<td>
						<div class="gui-checkbox-wrapper">
							<input id="sign_in_option" class="gui-checkbox" type="checkbox" name="option" checked="checked" />
						</div>
					</td>
				</tr>
				<tr>
					<td>
						<div class="button-column">
							<a id="add-node-button" class="gui-point-button gui-text-bold right">
		                    	<span class="gui-button-msg">Sign in</span>
		                	</a>
						</div>
					</td>
				</tr>
			</table>
		</div>
	</div>
    
    <div id="tooltips" class="hide">
        <div id="inner-tooltips">
            <table><tr>
                <td id="display-tips" class="gui-text"></td>
            </tr></table>
        </div>
    </div>
    
    <script src="/kesto/ui/js/jquery-1.7.min.js"></script>
    <script src="/kesto/ui/js/jquery-ui-1.8.16.custom.min.js"></script>
    <script src="/kesto/ui/js/jquery.lightbox_me-2.3.js"></script>
    <script src="/kesto/ui/js/jquery.cookie-1.2.js"></script>
    <script src="/kesto/ui/js/menu.js"></script>
</body>
</html>
